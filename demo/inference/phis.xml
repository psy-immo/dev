	<rule name="serial/parallel-swap">
		<premise id="ab">
			<predicate>is (connected in parallel|serial connected) with</predicate>
		</premise>
		<infer>
			<subject id="ab" source="object"/>
			<predicate id="ab" source="predicate"/>
			<object id="ab" source="subject"/>
		</infer>
