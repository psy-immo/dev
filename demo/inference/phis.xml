		<constraint>
			<predicate id="ab"/>
			<predicate id="bc"/>
		</constraint>
		<constraint>
			<object id="ab"/>
			<subject id="bc"/>
		</constraint>
		<infer>
			<subject id="ab" source="subject"/>
			<predicate id="ab" source="predicate"/>
			<object id="bc" source="object"/>
		</infer>
	</rule>
	
